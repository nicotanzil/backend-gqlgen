input Login {
    accountName: String!
    password: String!
}

extend type Mutation {
    login(input: Login): TokenDetail!
}

type AccessDetails {
    accessUuid: String!
    userId: Int!
}

type TokenDetail {
    accessToken: String!
    refreshToken: String!
    accessUuid: String!
    refreshUuid: String!
    accessExpires: Int!
    refreshExpires: Int!
}